-                }
-            } catch (IOException e) {
-                e.printStackTrace();
-            }
-            if (process != null) {
-                process.destroy();
-            }
-        }
-        return new CommandResult(result, successMsg == null ? null
-                : successMsg.toString(), errorMsg == null ? null
-                : errorMsg.toString());
-    }
